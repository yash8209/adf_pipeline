{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "yashpdatafactory"
		},
		"productblob1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'productblob1'"
		},
		"productblob2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'productblob2'"
		},
		"productdb_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'productdb'"
		},
		"productsserver_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'productsserver'"
		},
		"productdb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "yashpserver2.database.windows.net"
		},
		"productdb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "yashpdatabase"
		},
		"productdb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "yashp"
		},
		"productsserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "yashpserver2.database.windows.net"
		},
		"productsserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "yashpdatabase"
		},
		"productsserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "yashp"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "yashpdatafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"productblob1_connectionString": {
						"value": "[parameters('productblob1_connectionString')]"
					},
					"productblob2_connectionString": {
						"value": "[parameters('productblob2_connectionString')]"
					},
					"productdb_password": {
						"value": "[parameters('productdb_password')]"
					},
					"productsserver_password": {
						"value": "[parameters('productsserver_password')]"
					},
					"productdb_properties_typeProperties_server": {
						"value": "[parameters('productdb_properties_typeProperties_server')]"
					},
					"productdb_properties_typeProperties_database": {
						"value": "[parameters('productdb_properties_typeProperties_database')]"
					},
					"productdb_properties_typeProperties_userName": {
						"value": "[parameters('productdb_properties_typeProperties_userName')]"
					},
					"productsserver_properties_typeProperties_server": {
						"value": "[parameters('productsserver_properties_typeProperties_server')]"
					},
					"productsserver_properties_typeProperties_database": {
						"value": "[parameters('productsserver_properties_typeProperties_database')]"
					},
					"productsserver_properties_typeProperties_userName": {
						"value": "[parameters('productsserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}